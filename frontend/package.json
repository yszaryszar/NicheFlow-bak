    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }

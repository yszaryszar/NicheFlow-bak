{
  "name": "nicheflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build:dev": "dotenv -e .env.development -- next build",
    "build:prod": "dotenv -e .env.production -- next build",
    "start:dev": "dotenv -e .env.development -- next start",
    "start:prod": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "standard-version",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "antd": "^5.0.0",
    "axios": "^1.6.0",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intl": "^7.1.6",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.2.4",
    "cz-git": "^1.11.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.20.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.2",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}

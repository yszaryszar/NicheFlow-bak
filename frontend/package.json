{
  "name": "nicheflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build:dev": "dotenv -e .env.development -- next build",
    "build:prod": "dotenv -e .env.production -- next build",
    "start:dev": "dotenv -e .env.development -- next start",
    "start:prod": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "standard-version",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "antd": "^5.0.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-intl": "^6.6.2",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "commitizen": "^4.2.4",
    "cz-git": "^1.11.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}

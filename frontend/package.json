{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export HTTPS_PROXY=http://127.0.0.1:7897 HTTP_PROXY=http://127.0.0.1:7897 && dotenv -e .env.development -- next dev --turbo",
    "build:dev": "dotenv -e .env.development -- next build",
    "build:prod": "dotenv -e .env.production -- next build",
    "start:dev": "dotenv -e .env.development -- next start",
    "start:prod": "dotenv -e .env.production -- next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "standard-version",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@clerk/localizations": "^3.10.5",
    "@clerk/nextjs": "^6.11.2",
    "@clerk/themes": "^2.2.17",
    "@hookform/resolvers": "^4.0.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "@types/js-cookie": "^3.0.6",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^24.2.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-axiom": "^1.9.1",
    "next-i18next": "^15.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "react-intl": "^7.1.6",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@clerk/types": "^4.46.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@shadcn/ui": "^0.0.4",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.2.4",
    "cz-git": "^1.11.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.20.0",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
